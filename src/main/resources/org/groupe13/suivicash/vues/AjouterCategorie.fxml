<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.groupe13.suivicash.AjouterCategorieController"
      prefHeight="300.0" prefWidth="400.0"> <!-- Ajoutez ces lignes pour définir la hauteur et la largeur de la fenêtre -->
    <children>
        <Label text="Nom de la catégorie:" />
        <TextField fx:id="nomCategorieTextField" />
        <HBox spacing="10">
            <Button text="Ajouter" onAction="#handleAjouterClick" />
            <Button text="Annuler" onAction="#handleAnnulerClick" />
        </HBox>
    </children>
</VBox>
