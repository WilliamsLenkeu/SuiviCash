<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.1" xmlns:fx="http://javafx.com/fxml/1"
            prefWidth="806.0" prefHeight="518.0"
            fx:controller="org.groupe13.suivicash.RevenuController">

    <Button fx:id="AjouterRevenuButton" layoutX="24.0" layoutY="14.0" onAction="#handleAjouterRevenuClick" text="Ajouter Revenu" />

    <TableView fx:id="revenusTableView" layoutX="24.0" layoutY="59.0" prefWidth="755.0" prefHeight="400.0">
        <columns>
            <TableColumn fx:id="idCol" prefWidth="100.0" text="ID" />
            <TableColumn fx:id="montantCol" prefWidth="100.0" text="Montant" />
            <TableColumn fx:id="dateCol" prefWidth="100.0" text="Date" />
            <TableColumn fx:id="descriptionCol" prefWidth="250.0" text="Description" />
            <TableColumn fx:id="nomBanqueCol" prefWidth="150.0" text="Nom Banque" />
        </columns>
    </TableView>

</AnchorPane>